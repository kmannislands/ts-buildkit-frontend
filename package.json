{
  "name": "tap-parser-combinator",
  "version": "0.0.1",
  "description": "Experimental approach to parser TAP version 13",
  "main": "dist/index.js",
  "type": "module",
  "keywords": [
    "TAP",
    "node:test",
    "parser",
    "parser combinator"
  ],
  "author": "Kieran Mann <kieranjarrettmann@gmail.com>",
  "license": "MIT",
  "scripts": {
    "compile": "tsc -b --verbose",
    "test": "node --watch ./dist",
    "generate": "protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --proto_path=./vendor --ts_proto_out=./src --ts_proto_opt=esModuleInterop=true,importSuffix=.js ./vendor/moby-buildkit/ops.proto"
  },
  "devDependencies": {
    "@types/node": "^18.14.1",
    "ts-proto": "^1.140.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "long": "^5.2.1",
    "protobufjs": "^7.2.2"
  }
}
